# Q3 answer template

def solution(arr, divisor):
    answer = []
    count = 0
    for i in range(len(arr)):
        if arr[i] % divisor == 0 :
            answer.append(arr[i])
            count+=1
    if count == 0 :
        answer.append(-1)
    answer.sort()
    return answer

arr = [3,2,6]
divisor = 10
answer = solution(arr, divisor)
print(answer)
